pico-8 cartridge // http://www.pico-8.com
version 43
__lua__
scr=0 --score
st="intro" --game state
sp=12 --speed
ps=sp*1.5 --pause between ans and qst

function _init()
	pt={28,33,31,24} --pitches
	qst:i()
	--qst:a()
	--qst:pl()
end

function _update()
	if st=="intro" then
		intro:upd()
	end
	if st=="play" then 
		qst:pl()
	end
	if st=="listen" then
		dpad:upd()
	end
	if st=="gmo" then
	 gmo:upd()
	end
end

function _draw()
	cls(5)
	if (st=="intro") intro:drw()
	if st=="play" or st=="listen" then 
		dpad:drw()
	end
	gmo:drw()
	if (st!="intro") fdb:drw()
end


