--question
qst={
	q={1},
	qs=1,
	pi=1, --play index
	pz=0, --play zero time
	pt=0, --play time
	i=function(s)--init
	 qst.q={1}
	 qst.q[1]=flr(rnd(4))+1
		qst.pt=0
		qst.pi=1
	end,
	r=function(s) --reset
	 --st="play"
		s.pt=0
		s.pi=1
 end,
	a=function(s)
		s.q[#s.q+1]=flr(rnd(4))+1
	end,
	pl=function(s)
		if s.pi>#s.q then
			ans:i()
		end
		if s.pt%sp==0 and s.pi<=#s.q then
			snd(s.q[s.pi])
			s.pi+=1
		end
		s.pt+=1
	end
	}
	
	
	
