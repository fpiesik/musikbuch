--answer
ans={
	a={},
	df=0,
	tm=0,
	i=function(s)
		st="listen"
		s.a={}
	end,
	rec=function(s,b)
		if #ans.a<#qst.q then
			add(s.a,b)
			s:chk(#ans.a)
			snd(b)
		end

	end,
	chk=function(s,i)
 	s.df=qst.q[i]-ans.a[i] 	
 	if s.df!=0 then
 		st="gmo"
 	else
 		scr=scr+1
 	end
 end
}
				
	
	
